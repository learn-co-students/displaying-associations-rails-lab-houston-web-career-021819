<h1>All Artists:</h1>
  <ul>
  
      <% @artists.each do |a|%>
   <li>
        <%= link_to a.name, artist_path(a.id) %>
        <%= "#{a.name} - #{a.song_count} songs" %>
        <ul>
           <% @songs.each do |s| %>
              <% if s.artist_id == a.id %>
                <li> <%= link_to s.title, song_path(s.id) %> </li>
              <% end %>
            <% end %>
        </ul>
    </li>
      <% end %>
  </ul>